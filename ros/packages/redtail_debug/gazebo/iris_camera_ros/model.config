<?xml version="1.0"?>

<model>
  <name>Iris with Standoffs and Camera LiftDrag ArduCopter Plugins + Small fixed angle Gimbal 2d + ROS video stream</name>
  <version>1.0</version>
  <sdf version="1.6">model.sdf</sdf>

  <author>
    <name>Fadri Furrer</name>
    <email>fadri.furrer@mavt.ethz.ch</email>
  </author>
  <author>
    <name>Michael Burri</name>
  </author>
  <author>
    <name>Mina Kamel</name>
  </author>
  <author>
    <name>Janosch Nikolic</name>
  </author>
  <author>
    <name>Markus Achtelik</name>
  </author>

  <maintainer email="hsu@osrfoundation.org">john hsu</maintainer>

  <!-- modidied by Sepehr MohaimenianPour,smohaime@sfu.ca to change the gimall model-->

  <description>
    starting with iris_with_standoffs
    add LiftDragPlugin
    add ArduCopterPlugin
    attach gimbal_small_2d model with GimbalSmall2dPlugin
    camera stream published on ROS
  </description>
  <depend>
    <model>
      <uri>model://gimbal_small_2d_fixed_ros</uri>
      <version>1.0</version>
    </model>
    <model>
      <uri>model://iris_with_standoffs</uri>
      <version>1.0</version>
    </model>
  </depend>
</model>
